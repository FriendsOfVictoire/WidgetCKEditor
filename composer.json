{
    "name": "friendsofvictoire/ckeditor-widget",
    "type": "symfony-bundle",
    "description": "Victoire DCMS CKEditor widget",
    "keywords": [
        "DCMS",
        "Victoire",
        "widget",
        "FriendsofVictoire",
        "CKEditor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name" : "Leny Bernard",
            "email": "leny@victoire.io"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.8|~3.0",
        "egeloen/ckeditor-bundle" : "~4.0.1",
        "victoire/victoire": "~1.5"
    },
    "autoload": {
        "psr-0": {
            "Victoire\\Widget\\CKEditorBundle": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire/Widget/CKEditorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
